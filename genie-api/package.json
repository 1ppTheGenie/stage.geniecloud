{
	"name": "genie-api",
	"version": "1.0.0",
	"description": "Endpoints and associated functionality to create a cloud version of GenieHub",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"preaws": "node build.mjs && tar -a -c -f genie-api.zip ./build/index.js",
		"create": "aws lambda create-function --function-name GenieAPI --runtime nodejs18.x --zip-file fileb://genie-api.zip --handler ./build/index.api ",
		"add-layers": "aws lambda update-function-configuration --function-name GenieAPI ",
		"aws": "aws lambda update-function-code --function-name GenieAPI --zip-file fileb://genie-api.zip"
	},
	"keywords": [],
	"author": "Dynamic Array Ltd",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.347.1",
		"@aws-sdk/client-sqs": "^3.347.1",
		"@qrcode-js/node": "^1.1.0",
		"jstoxml": "^3.2.7",
		"luxon": "^3.3.0"
	},
	"devDependencies": {
		"esbuild": "^0.18.4",
		"esbuild-plugin-alias-path": "^2.0.2",
		"esbuild-plugin-path-alias": "^1.0.7",
		"global": "^4.4.0"
	}
}
